prototype(Neos.Neos:Page) {
    head.multicolumn = Neos.Fusion:Tag {
        tagName = 'link'
        attributes {
            rel = 'stylesheet'
            href = Neos.Fusion:ResourceUri {
                path = 'resource://JvMTECH.MultiColumn/Public/MultiColumn.css'
            }
        }
        @if.isEnabled = ${Configuration.setting('JvMTECH.MultiColumn.includeCss') && Neos.Node.inBackend(node) ? true : false}
        @position = 'end'
    }
}
